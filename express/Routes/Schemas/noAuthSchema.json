{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "testuser"
            ],
            "minLength": 4,
            "maxLength": 25
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "badpassword"
            ],
            "minLength": 6,
            "maxLength": 25
        }
    },
    "examples": [
        {
            "username": "testuser",
            "password": "badpassword"
        }
    ]
}